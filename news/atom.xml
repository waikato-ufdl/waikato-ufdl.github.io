---
layout: none
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>User-friendly Deep-learning News</title>
	<link href="https://waikato-ufdl.github.io/news/atom.xml" rel="self"/>
	<link href="https://waikato-ufdl.github.io/news"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://waikato-ufdl.github.io/news</id>
	<author>
		<name>UFDL Team</name>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://waikato-ufdl.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://waikato-ufdl.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
